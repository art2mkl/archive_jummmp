{{ form_start(form) }}
<div class="box">
	<div class="mb-3">
		{{ form_label(form.icon, 'Choisis ton icone', {'label_attr': {'class': 'form-label'}}) }}
		<div>{{ form_widget(form.icon, {'attr': {'class': 'form-select'}}) }}</div>
	</div>

	<div class="mb-3">
		{{ form_label(form.hobbyName, 'Ton loisir', {'label_attr': {'class': 'form-label'}}) }}
		<div>{{ form_widget(form.hobbyName, {'attr': {'class': 'form-control', 'placeholder' : 'Le nom de ton hobby'}}) }}</div>
	</div>


	<button class="btn">
		<i class="far fa-save"></i>
	</button>
</div>
{{ form_end(form) }}
